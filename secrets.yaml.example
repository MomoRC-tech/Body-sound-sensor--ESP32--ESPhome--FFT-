# ESP32 Vibration Sensor - WiFi and Security Configuration
# IMPORTANT: Copy this file to 'secrets.yaml' and fill in your actual values
# Never commit secrets.yaml to git!

# WiFi Configuration
wifi_ssid: "MOMOWLAN"
wifi_password: "YOUR_WIFI_PASSWORD_HERE"

# API encryption key (generate with: openssl rand -base64 32)
api_encryption_key: "YOUR_32_BYTE_BASE64_KEY_HERE"

# OTA password (for over-the-air updates)
ota_password: "YOUR_OTA_PASSWORD_HERE"
